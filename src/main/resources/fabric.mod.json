{
  "schemaVersion": 1,
  "id": "vivecraftfabric",
  "version": "${version}",

  "name": "Vivecraft Fabric",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Me!"
  ],

  "license": "CC0-1.0",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.vivecraftfabric.VivecraftFabric"
    ]
  },
  "mixins": [
    "vivecraftfabric.mixins.json",
    "vivecraftfabric.sodium.mixins.json",
    "vivecraftfabric.iris.mixins.json"
  ],
  "accessWidener": "vivecraftfabric.accesswidener",
  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.18.x",
    "java": ">=17"
  },
  "suggests": {
    "another-mod": "*"
  }
}
